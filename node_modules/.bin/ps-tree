#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_ps-tree@1.2.0@ps-tree/bin/ps-tree.js" "$@"
  ret=$?
else 
  node  "$basedir/../_ps-tree@1.2.0@ps-tree/bin/ps-tree.js" "$@"
  ret=$?
fi
exit $ret
